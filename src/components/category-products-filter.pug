.category-products-filter
  h4.title Filters
  .filters
    //price filter
    .price-filter.clearfix
      p.filter-label Price
      .range-from.number-input
        input#price-from.form-control(type='number' value='99')
        .counter
          span.fa.fa-angle-up.increment
          span.fa.fa-angle-down.decrement  
      .range-to.number-input
        input#price-to.form-control(type='number' value='999')
        .counter
          span.fa.fa-angle-up.increment
          span.fa.fa-angle-down.decrement 
    //category filter
    .product-category-filter.open.clearfix
      p.filter-label Manufacturers <span data-toggle="collapse" data-target="#collapseManufacturer" class="fa fa-plus-circle toggle-list">
      #collapseManufacturer.collapse.in
        ul.category-list
          li
            .checkbox
              input#checkbox1(type='checkbox')
              label(for='checkbox1') Kashmir Exotic 
            .checkbox
              input#checkbox2(type='checkbox')
              label(for='checkbox2') Kashur
            .checkbox
              input#checkbox3(type='checkbox')
              label(for='checkbox3') Pine
            .checkbox
              input#checkbox4(type='checkbox')
              label(for='checkbox4') RV Spices 
